{
  "name": "vue-project-accelerator",
  "version": "0.4.1",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spinningideas/vue-project-accelerator.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "buildghpages": "vite build --base=/vue-project-accelerator/",
    "deployghpages": "gh-pages -d dist",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@kyvg/vue3-notification": "^3.0.2",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.2"
  },
  "devDependencies": {
    "@mdi/font": "^7.3.67",
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.9.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "gh-pages": "^6.1.0",
    "install": "^0.13.0",
    "npm": "^10.2.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "typescript": "5.2.2",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.22"
  }
}
